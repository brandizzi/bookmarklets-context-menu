{

  "manifest_version": 2,
  "version": "1.0",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "homepage_url": "https://github.com/mems/bookmarklets-context-menu",

  
  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "bookmarks",
    "contextMenus",
    "activeTab",
    "clipboardWrite",
    "storage",
    "<all_urls>"
  ],

  "options_ui": {
      "page": "options/options.html",
      "browser_style": true
  }
}